{
  "manifest_version": 3,
  "name": "Turfia 的导航首页 (Turfia's New Homepage)",
  "version": "1.2",
  "description": "无联网功能的标签页",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "favicon"
  ],
  "host_permissions": [
    "https://favicone.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    },
    "default_title": "TFNP"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icons/icon16.PNG"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}